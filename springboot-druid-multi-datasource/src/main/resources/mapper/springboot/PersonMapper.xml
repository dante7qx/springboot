<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.dante.springboot.mapper.springboot.PersonMapper">
	<select id="queryPersons" resultType="PersonBO">
		select t.id, t.name, t.age, t.address, t.updateBy, t.updateDate from T_Person t order by t.age
	</select>
	
</mapper>