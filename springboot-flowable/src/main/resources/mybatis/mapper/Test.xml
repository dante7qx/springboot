<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.dante.springboot.mybatis.mapper.TestMapper">

	<resultMap id="TestResult" type="TestVO">
		<id column="id" property="id" />
		<result column="name" property="name" />
	</resultMap>


	<select id="queryDeployProcess" resultMap="TestResult">
		select t.deploy_process_id as id, t.process_name as name from t_deploy_process t
	</select>
</mapper>