	<artifactId>springboot-office</artifactId>
	<packaging>jar</packaging>
	<name>springboot-office</name>
	<description>Dante Springboot Office</description>

	<properties>
		<iceblue-spire.version>4.11.3</iceblue-spire.version>
		<aspose.version>15.8.0</aspose.version>
		<document4j.version>1.1.7</document4j.version>
